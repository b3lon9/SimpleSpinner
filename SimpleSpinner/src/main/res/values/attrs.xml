<resources>
    <declare-styleable name="SimpleSpinner">
        <attr name="popup_visibility" format="boolean" />
        <attr name="popup_width" format="dimension">
            <!--default width-->
            <enum name="match_parent" value="-1" />
        </attr>
        <attr name="popup_height" format="dimension">
            <!--default height-->
            <enum name="match_parent" value="-1" />
        </attr>
    </declare-styleable>
</resources>